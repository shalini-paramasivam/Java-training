class Datatypes{
	public static void main(String args[])
	{
		char a = 'S';
		int i = 77;
		byte b = 8;
		double d = 7.9952853;
		float f = 6.693393f;
		long l = 81818;

		System.out.println("char: " + a);
		System.out.println("integer: " + i);
		System.out.println("byte: " + b);
		System.out.println("float: " + f);
		System.out.println("double: " + d);
		System.out.println("long: " + l);
	}
}
